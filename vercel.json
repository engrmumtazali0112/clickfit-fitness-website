      "src": "/upload_images/(.*)",
      "dest": "/upload_images/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/upload",
      "dest": "/server/server.js"
    },
    {
      "src": "/delete/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/health",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.js"
    }
